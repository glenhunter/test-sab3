{
  "name": "@sonatype/sonatype-application-builder",
  "version": "0.0.0",
  "description": "Sonatype Application Template",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sonatype/sonatype-application-builder.git"
  },
  "author": "Sonatype, Inc.",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "npm-run-all lint webpack-dev-server",
    "build": "npm-run-all lint webpack copy-package-json copy-assets copy-index",
    "copy-package-json": "cpx package.json dist",
    "copy-assets": "cpx \"src/main/assets/**/*\" dist/assets",
    "copy-index": "cpx \"src/main/index.html\" dist",
    "jest": "jest",
    "lint": "eslint --ignore-pattern __tests__ --ignore-pattern __mocks__ \"src/**/*.{js,ts}{,x}\"",
    "lint-tests": "eslint -c .eslintrc-tests \"src/**/{__tests__,__mocks__}/**/*.{js,ts}{,x}\"",
    "test": "npm-run-all lint-tests jest",
    "test-watch": "jest --watch",
    "test-watch-debug": "node --inspect-brk node_modules/.bin/jest --runInBand --watchAll",
    "webpack": "webpack --env production --mode=production",
    "webpack-dev-server": "webpack serve"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.23",
    "@types/react": "^16.0.0",
    "@types/react-dom": "^16.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-syntax-highlighter": "^13.5.1",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "copy-modules-webpack-plugin": "^2.2.1",
    "cpx": "^1.5.0",
    "css-loader": "^5.2.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.29.0",
    "eslint-plugin-react": "^7.24.0",
    "file-loader": "^6.2.0",
    "jest": "^27.0.5",
    "jest-enzyme": "^7.1.2",
    "jest-junit": "^12.2.0",
    "mini-css-extract-plugin": "^1.6.0",
    "npm-run-all": "^4.1.5",
    "raw-loader": "^4.0.2",
    "react-redux": "^7.2.4",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "resolve-url-loader": "^4.0.0",
    "sass-loader": "^12.1.0",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.4",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@sonatype/react-shared-components": "^7.1.3",
    "axios": "^0.21.1",
    "node-sass": "^6.0.0",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-router-dom": "^5.2.0"
  }
}
